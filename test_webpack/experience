            $('#iframegwcontact').attr('src', 'http://syb.maimiaotech.com/gwcontact'+location.search);
        }*/

        function getCookie(name) {
            var cookieValue = null;
            if(document.cookie && document.cookie != '') {
                var cookies = document.cookie.split(';');
                for(var i = 0; i < cookies.length; i++) {
                    var cookie = jQuery.trim(cookies[i]);
                    // Does this cookie string begin with the name we want?
                    if(cookie.substring(0, name.length + 1) == (name + '=')) {
                        cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                        break;
                    }
                }
            }
            return cookieValue;
        }
        /*
            约定 $('.nav-content')的id 为 id
            则其中的iframe的id为 'iframe' + id
            nav的href 为 '#' + id
        */
        var NAME = {
            0: '优化规则包',
            1: '优化规则包',
            //2: '创意优化',
            2: '自动选词',
            3: '手动淘词',
            4: '优化出价',
        };
        var LOG = {
            0: 'yhgzfx1',
            1: 'yhgzfx1',
            //2: 'creative',
            2: 'xuanci',
            3: 'taoci',
            4: 'yhcj',
        }
        $(".nav-tab").click(function(e) {
            e.preventDefault()

            var id = $(this).attr('href').substring(1), index = $(this).index();
            if(!$('#iframe' + id).attr('src')){
                $('#iframe' + id).attr('src', 'http://syb.maimiaotech.com/' + id +location.search);
            }
            // if(index > 1){
            //     $('.r-info-wrap3').hide();
            // }else{
            //     $('.r-info-wrap3').show();
            // }
            if($.inArray(index, [2,3,4,5]) > -1){//不要展示
                $('.r-info-wrap0,.order-btn-orange').hide();
            }else{
                $('.r-info-wrap0,.order-btn-orange').show();
            }
            $('.order-btn-orange').attr('href','https://fuwu.taobao.com/ser/detail.html?spm=a1z13.1113643.0.0.BjCoAX&service_code=ts-1796606&tracelog='+LOG[index]).find('.apply-name').html(NAME[index]);
            $(".nav-tab").removeClass('active').filter('[href="#' + id + '"]').addClass('active')
            $(".nav-content").removeClass('active').filter('#' + id).addClass('active')
        });
        //tab选中
        if(location.hash){
            $(".nav-tab[href="+location.hash+"]").trigger('click');
        }else{//默认展示第一个tab
            $(".nav-tab:eq(0)").trigger('click');
        }
        //报表下载教程
        $('.show-guide-btn').click(function(e){
            e.preventDefault();
            $('#mask, #guide_pop').show();
        });
        $('.close').click(function(e){
            e.preventDefault();
            $('#mask, #guide_pop').hide();
        });
        //联系我们
        $('.toMin, .min-contact-wrap').click(function(e) {
            $('.min-contact-wrap, .contact-wrap').toggle();
        });
        $('.toTop').click(function(e) {
            $("html,body").animate({scrollTop:"0px"},200);
        });
        window.addEventListener('message', function(e){
            if(e.origin.indexOf('maimiaotech.com') > -1){
                var params = e.data.split('#')
                if(params[0] === 'iframeHeight'){
                    $('#iframe' + params[1]).css('height',params[2])
                }else if(params[0] === 'hideMask'){
                    $('#mask, #get_contact_pop').hide();
                }else if(params[0] === 'price'){
                    $('.load-hide').hide();
                }
            }
        })
    })
    </script>

    <!--百度统计代码-->
    <script>
        var _hmt = _hmt || [];
        (function() {
            var hm = document.createElement("script");
            hm.src = "//hm.baidu.com/hm.js?cdf011a9ef990ca0279ffdab9ccb5605";
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(hm, s);
        })();
        $('body').delegate('a, button', 'click', function(){
            if($(this).attr('p-categorie') && $(this).attr('p-action')) {
                window._hmt.push(['_trackEvent', $(this).attr('p-categorie'), $(this).attr('p-action')]);
            }
        });
    </script>
</body>
