<script type="text/javascript" src="./static/js/jquery.min.js"></script>
<script type="text/javascript">
$(function(){
    //ref来源始终带着
    var ref_str;
    var LINK_RE = /.*(?:\&|\?)ref=(\w+)\&?.*/;
    if(location.href.indexOf('ref=') > -1){
        ref_str = location.href.match(LINK_RE);
        if(ref_str[1]){//每个导航都带上ref
            $.each($('.menu-nav a'), function(i, el){
                $(el).attr('href', $(el).attr('href')+'?ref='+ref_str[1]);
            });
        }
    }
    $(".nav-left a").click(function(e) {
        e.preventDefault()
        var index = $(this).parent().index()
        $(".nav-left li").removeClass('active').eq(index).addClass('active')
        $(".right-content li").hide().eq(index).show()
    });

    $(".nav-tab").click(function(e) {
        e.preventDefault()

        var index = $(this).index()
        $(".nav-tab").removeClass('active').eq(index).addClass('active')
        $(".nav-content").hide().eq(index).show()
    });
})
</script>

